#BACKUP_APP_ADJ
PRESET_0_ADJ_6=5

#HOME_APP_ADJ
PRESET_0_ADJ_7=6

#HIDDEN_APP_MIN
PRESET_0_ADJ_8=7

#EMPTY_APP_ADJ
PRESET_0_ADJ_9=15


## Process Class memory thresholds

#FOREGROUND_APP_MEM
PRESET_0_MEM_1=2048

#VISIBLE_APP_MEM
PRESET_0_MEM_2=3072

#PERCEPTIBLE_APP_MEM
PRESET_0_MEM_3=4096

